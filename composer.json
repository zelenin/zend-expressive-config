{
    "name": "zelenin/zend-expressive-config",
    "description": "Zend Expressive config manager",
    "keywords": [
        "zend",
        "expressive",
        "config"
    ],
    "homepage": "https://github.com/zelenin/zend-expressive-config",
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksandr Zelenin",
            "email": "aleksandr@zelenin.me",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/zelenin/zend-expressive-config/issues",
        "source": "https://github.com/zelenin/zend-expressive-config"
    },
    "require": {
        "php": ">=7.0",
        "symfony/yaml": "~2.8 || ~3.0",
        "doctrine/annotations": "~1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Zelenin\\Zend\\Expressive\\Config\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zelenin\\Zend\\Expressive\\Config\\Test\\": "tests"
        }
    }
}
